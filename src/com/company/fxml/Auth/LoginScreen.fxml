<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.company.fxml.Auth.LoginController">
    <TextField fx:id="login" layoutX="224.0" layoutY="134.0" promptText="Введите логин"/>
    <Button fx:id="btn" layoutX="274.0" layoutY="208.0" mnemonicParsing="false" onAction="#onClick" text="Войти"/>
    <PasswordField fx:id="password" layoutX="224.0" layoutY="174.0" promptText="Введите пароль"/>
    <Button fx:id="register" layoutX="256.0" layoutY="326.0" mnemonicParsing="false" onAction="#onRegister"
            text="Регистрация"/>
    <Text fx:id="error" fill="RED" layoutY="261.0" strokeType="OUTSIDE" strokeWidth="0.0" textAlignment="CENTER"
          wrappingWidth="600.0"/>
</AnchorPane>
