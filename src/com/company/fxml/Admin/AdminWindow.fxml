<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="591.0" prefWidth="1110.0" xmlns="http://javafx.com/javafx/11.0.1"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.company.fxml.Admin.AdminWindowController">
    <TableView fx:id="tableGroups" layoutX="55.0" layoutY="154.0" prefHeight="361.0" prefWidth="524.0">
        <columns>
            <TableColumn fx:id="tableColumnId" prefWidth="29.8487548828125" text="ID"/>
            <TableColumn fx:id="tableColumnName" prefWidth="121.8150691986084" text="Название"/>
            <TableColumn fx:id="tGroupSize" prefWidth="134.7227020263672" text="Число учеников"/>
            <TableColumn fx:id="tRemoveButton" prefWidth="76.63861083984375"/>
            <TableColumn fx:id="tShowButton" prefWidth="154.08407592773438"/>
        </columns>
    </TableView>
    <TableView fx:id="tableSubjects" layoutX="638.0" layoutY="154.0" prefHeight="361.0" prefWidth="409.0">
        <columns>
            <TableColumn fx:id="tSubjectID" prefWidth="34.68902587890625" text="ID"/>
            <TableColumn fx:id="tSubjectName" prefWidth="212.974853515625" text="Название"/>
            <TableColumn fx:id="tSubjectButton" prefWidth="157.31103515625"/>
        </columns>
    </TableView>
    <Button fx:id="addGroup" layoutX="466.0" layoutY="532.0" mnemonicParsing="false" onAction="#onAddGroup"
            text="Добавить класс"/>
    <Button fx:id="addSubject" layoutX="905.0" layoutY="532.0" mnemonicParsing="false" onAction="#onAddSubject"
            text="Добавить предмет"/>
    <Text layoutX="478.0" layoutY="40.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Здравствуйте!"
          textAlignment="CENTER">
        <font>
            <Font size="24.0"/>
        </font>
    </Text>
    <Text layoutX="55.0" layoutY="85.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Имя:"/>
    <Text fx:id="name" layoutX="100.0" layoutY="85.0" strokeType="OUTSIDE" strokeWidth="0.0"/>
    <TextField fx:id="groupInput" layoutX="55.0" layoutY="532.0" prefHeight="25.0" prefWidth="372.0"
               promptText="Введите класс"/>
    <TextField fx:id="subjectInput" layoutX="638.0" layoutY="532.0" prefHeight="25.0" prefWidth="245.0"
               promptText="Введите название предмета"/>
    <Button fx:id="goBack" layoutX="992.0" layoutY="17.0" mnemonicParsing="false" onAction="#onGoBack" text="Выйти"/>
</AnchorPane>
